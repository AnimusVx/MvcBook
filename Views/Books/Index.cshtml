@model IEnumerable<MvcWorkspace.Models.DenemeData>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <table>
        <tr>
            @foreach (var item in Model)
            {
                <td>@item.Id</td>
                <td>@item.Adi</td>
                <td>@item.SayfaSayisi</td>
            }
        </tr>
    </table>
</div>
